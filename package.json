{
    "name": "svg-font-viewer",
    "displayName": "svg-font-viewer",
    "description": "Preview your svg font. Open command pelete `SVG Font Preview` or `cmd+shift+v`",
    "version": "0.0.1",
    "publisher": "nkokhelox",
    "preview": true,
    "engines": {
        "vscode": "^1.24.0"
    },
    "license": "MIT",
    "keywords": [
        "svg",
        "font",
        "preview"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.svgFontPreview"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "svg",
                "extensions": [
                    ".svg"
                ]
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "extension.svgFontPreview",
                    "when": "resourceLangId == svg"
                }
            ],
            "editor/title": [
                {
                    "command": "extension.svgFontPreview",
                    "when": "resourceLangId == svg"
                }
            ]
        },
        "commands": [
            {
                "command": "extension.svgFontPreview",
                "title": "SVG Font Preview"
            }
        ],
        "keybindings": [
            {
                "command": "extension.svgFontPreview",
                "when": "editorLangId == svg",
                "key": "ctrl+shift+v",
                "mac": "cmd+shift+v"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/xmldom": "^0.1.29",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43"
    },
    "dependencies": {
        "lodash": "^4.17.10",
        "xmldom": "^0.1.27"
    }
}