{
    "name": "svg-font-viewer",
    "displayName": "svg-font-viewer",
    "description": "preview your svg font. Open command pelete `PISD Previewer` or `cmd+shift+v`",
    "version": "0.0.1",
    "publisher": "nkokhelox",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pisdPreviewer"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                //cmd+shift+v
                "command": "extension.pisdPreviewer",
                "title": "PISD Previewer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/xmldom": "^0.1.29",
        "lodash": "^4.17.10",
        "xmldoc": "^1.1.0",
        "xmldom": "^0.1.27"
    }
}
